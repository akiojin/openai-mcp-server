{
  "mcpServers": {
    "openai": {
      "command": "npx",
      "args": [
        "."
      ]
    },
    "google-search": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "mcp-gemini-google-search"
      ],
      "env": {}
    },
    "context7": {
      "type": "sse",
      "url": "https://mcp.context7.com/sse"
    },
    "markitdown": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "markitdown-mcp"
      ],
      "env": {}
    },
    "model-hub": {
      "command": "npx",
      "args": ["@akiojin/model-hub-mcp"]
    }
  }
}
